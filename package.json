{
  "name": "cek-khodam",
  "version": "0.1.2",
  "description": "Mau cek khodam? cek disini aja sama liat personality kamu hehe",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cryeed/app-cek-khodam.git"
  },
  "build": {
    "appId": "com.noochromedev.appcekkhodam",
    "productName": "Applikasi Cek Khodam",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html",
      "node_modules/**/*",
      "package.json",
      "pacakge-lock.json",
      "preload/**/*",
      "public/**/*",
      "feather/**/*",
      "favicon.png"
    ],
    "win": {
      "icon": "./favicon.png",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "electron"
  ],
  "author": "Cryeed, NochromeDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cryeed/app-cek-khodam/issues"
  },
  "homepage": "https://github.com/cryeed/app-cek-khodam#readme",
  "devDependencies": {
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3"
  }
}
